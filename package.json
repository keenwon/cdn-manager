{
  "name": "cdn-manager",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "dev": "NODE_ENV=development DEBUG=* nodemon --ignore logs/ --ignore client/ -- --harmony app.js",
    "pro": "npm run build && NODE_ENV=production DEBUG=* node --harmony app.js",
    "build": "npm test && npm run clean && npm run build:js",
    "build:js": "NODE_ENV=production webpack --bail --progress --display-chunks --display-modules --config webpack.config.js",
    "clean": "rm -rf client/scripts",
    "test": "eslint --format 'node_modules/eslint-friendly-formatter' --ext .js,.vue client/src lib site *.js"
  },
  "author": "keenwon <semanwmj@yeah.net>",
  "license": "MIT",
  "dependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "css-loader": "^0.26.1",
    "fs-extra": "^1.0.0",
    "koa": "next",
    "koa-bodyparser": "next",
    "koa-compose": "next",
    "koa-convert": "^1.2.0",
    "koa-favicon": "next",
    "koa-pug": "^3.0.0-1",
    "koa-router": "^7.1.0",
    "koa-send": "^3.3.0",
    "koa-webpack-dev-middleware": "^1.4.4",
    "moment": "^2.16.0",
    "semver": "^5.3.0",
    "winston": "^2.3.0",
    "winston-daily-rotate-file": "^1.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.12.2",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-import": "^2.2.0",
    "nodemon": "^1.11.0",
    "vue": "^2.1.10",
    "vue-loader": "^10.1.0",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^2.2.1"
  }
}
